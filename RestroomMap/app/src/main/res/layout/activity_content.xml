<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/menu"
    android:weightSum="1"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/naver"
        android:layout_marginBottom="23dp"
        android:layout_marginEnd="12dp"
        android:layout_above="@+id/imageView15"
        android:layout_toStartOf="@+id/imageView16" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/facebook"
        android:layout_alignBottom="@+id/imageView15"
        android:layout_alignStart="@+id/imageView16" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/youtube"
        android:layout_alignTop="@+id/imageView16"
        android:layout_toEndOf="@+id/imageView16"
        android:layout_marginStart="12dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:srcCompat="@drawable/a"

        android:layout_alignBottom="@+id/imageView17"
        android:layout_alignEnd="@+id/imageView12" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/unnamed"
        android:layout_alignTop="@+id/imageView12"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/twitte"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/imageView14" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/hel"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="14dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>